# --- Prisma / Supabase Postgres ---
# DIRECT: folosit local pentru migrații (port 5432)
DATABASE_URL="postgresql://postgres:YOUR_PASSWORD@db.YOUR-PROJECT.supabase.co:5432/postgres?schema=public"

# POOLER (opțional): util la serverless; NU pentru migrații.
# Exemplu doar informativ (nu necesar acum):
# DATABASE_URL_POOLER="postgresql://YOUR_USER:YOUR_PASSWORD@aws-0-YOUR-REGION.pooler.supabase.com:6543/postgres?pgbouncer=true&connection_limit=1&schema=public"

# --- Supabase (client) ---
NEXT_PUBLIC_SUPABASE_URL="https://YOUR-PROJECT.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_ANON_KEY"

# --- Supabase (server-only, opțional) ---
# NECESAR doar pentru taskuri server-side privilegiate (ex: export GDPR)
SUPABASE_SERVICE_ROLE_KEY="YOUR_SERVICE_ROLE_KEY"

# --- Clerk Authentication ---
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY="YOUR_PUBLISHABLE_KEY"
CLERK_SECRET_KEY="YOUR_SECRET_KEY"